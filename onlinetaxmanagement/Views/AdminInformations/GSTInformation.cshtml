@model IEnumerable<onlinetaxmanagement.Models.GSTINformation>

@{
    ViewBag.Title = "GSTInformation";
}
@ViewBag.rege
@ViewData["reg"]
<h2>GSTInformation</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MRP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GST)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bill)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Registration.FirstName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MRP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GST)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bill)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Registration.FirstName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Gid }) |
            @Html.ActionLink("Details", "Details", new { id=item.Gid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Gid })
        </td>
    </tr>
}

</table>
